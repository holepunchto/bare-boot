    if (isImport && isCustomScheme(req)) return req
    if (this.builtins.has(req)) return req

    const self = this
    const runtimes = isImport ? this._importRuntimes : this._requireRuntimes

    return new Promise((resolve, reject) => {
      resolveModule(req, {
