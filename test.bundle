function isCustomScheme (str) {
  return /^[a-z][a-z0-9]+:/i.test(str)
}

function sniffJS (src) {
  const s1 = src.match(/"[^"]+"/ig)
  const s2 = src.match(/'[^']+'/ig)

  const entries = []

  if (s1) {
    for (const s of s1) {
      if (/\.(m|c)?js"$/.test(s)) {
        entries.push(s.slice(1, -1))
      }
    }
  }

  if (s2) {
    for (const s of s2) {
      if (/\.(m|c)?js'$/.test(s)) {
        entries.push(s.slice(1, -1))
      }
    }
  }

  return entries.filter(e => !isCustomScheme(e))
}
