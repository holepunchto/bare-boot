        isFile: (name, cb) => {
          self._isFile(name).then((yes) => cb(null, yes), cb)
        },
        isDirectory: (name, cb) => {
          self._isDirectory(name).then((yes) => cb(null, yes), cb)
        },
        readFile: (name, cb) => {
          self._readFile(name).then((buf) => cb(null, buf), cb)
